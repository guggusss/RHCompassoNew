[32m+[m[32mfunction funcao() {[m
[32m+[m	[32mdocument.getElementById("senhaUsuario").innerHTML = "Nova senha";[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mdocument.getElementById("senhaUsuario").addEventListener("click", function () {[m
[32m+[m	[32mfunction hasUppercase(char) {[m
[32m+[m		[32mvar regex = new RegExp('[A-Z]');[m
[32m+[m		[32mreturn regex.test(char);[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfunction asciiLength(ascii) {[m
[32m+[m		[32mreturn Math.floor(Math.random() * ascii.length);[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfunction asciiChar(ascii, i) {[m
[32m+[m		[32mreturn String.fromCharCode(Math.floor(Math.random() * (ascii[i][1] - ascii[i][0])) + ascii[i][0])[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfunction generatorPassword(pw_height) {[m
[32m+[m		[32mvar pw_height_default = pw_height || 8;[m
[32m+[m
[32m+[m		[32mwhile (0 < 1) {[m
[32m+[m			[32mvar ascii = [[m
[32m+[m				[32m[48, 57],[m
[32m+[m				[32m[64, 90],[m
[32m+[m				[32m[97, 122][m
[32m+[m			[32m];[m
[32m+[m			[32mvar ascii_special = [[m
[32m+[m				[32m[33, 33],[m
[32m+[m				[32m[35, 38],[m
[32m+[m				[32m[42, 46][m
[32m+[m			[32m];[m
[32m+[m			[32mvar temp_ascii;[m
[32m+[m			[32mvar roll_dice = Math.floor(Math.random() * pw_height_default);[m
[32m+[m			[32mvar password = new String();[m
[32m+[m			[32mvar finish_repeat = false;[m
[32m+[m			[32mfor (let index = 0; index < pw_height_default; index++) {[m
[32m+[m				[32mif (index === roll_dice) {[m
[32m+[m					[32mtemp_ascii = ascii_special;[m
[32m+[m				[32m} else {[m
[32m+[m					[32mtemp_ascii = ascii;[m
[32m+[m				[32m}[m
[32m+[m				[32mvar i = asciiLength(temp_ascii);[m
[32m+[m				[32mvar temp = asciiChar(temp_ascii, i);[m
[32m+[m				[32mif (i == 0) {[m
[32m+[m					[32mtemp = temp.toUpperCase();[m
[32m+[m				[32m}[m
[32m+[m				[32mpassword += temp;[m
[32m+[m				[32mfinish_repeat = true;[m
[32m+[m			[32m}[m
[32m+[m			[32mif (finish_repeat) {[m
[32m+[m				[32mbreak[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m		[32m$('#senhaUsuario').click(function(){[m
[32m+[m			[32m$('#jogaSenha').val(password);[m
[32m+[m		[32m});[m
[32m+[m		[32mreturn password;[m
[32m+[m	[32m}[m
[32m+[m	[32mgeneratorPassword();[m
[32m+[m[32m});[m
\ No newline at end of file[m
