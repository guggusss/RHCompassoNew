[32m+[m[32m<?php[m
[32m+[m[32mfunction removeMascAndFormatDate($datewhitMasc)[m
[32m+[m[32m{[m
[32m+[m[32m    $day = substr($datewhitMasc, -10, 2);[m
[32m+[m[32m    $mouth = substr($datewhitMasc, -7, 2);[m
[32m+[m[32m    $year = substr($datewhitMasc, -4, 4);[m
[32m+[m
[32m+[m[32m    return $year.'-'.$mouth.'-'.$day;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction formatDateApresentation($formatDate)[m
[32m+[m[32m{[m
[32m+[m[32m  $day = substr($formatDate, 8, 2);[m
[32m+[m[32m  $mouth = substr($formatDate, 5, 2);[m
[32m+[m[32m  $year = substr($formatDate, 0, 4);[m
[32m+[m
[32m+[m[32m  return $day.'/'.$mouth.'/'.$year;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m?>[m
