[32m+[m[32m      } else {[m[41m
[m
[32m+[m[32m          // else we'll just create one[m[41m
[m
[32m+[m[32m          tmpEl = document.createElement("div");[m[41m
[m
[32m+[m[41m
[m
[32m+[m[32m          // since we remove the element immedeately we'd actually not have to style it - but IE 11 prompts us to confirm the clipboard interaction and until you click the confirm button, the element would show. so: still extra stuff for IE, as usual.[m[41m
[m
[32m+[m[32m          tmpEl.style.opacity = 0;[m[41m
[m
[32m+[m[32m          tmpEl.style.position = "absolute";[m[41m
[m
[32m+[m[32m          tmpEl.style.pointerEvents = "none";[m[41m
[m
[32m+[m[32m          tmpEl.style.zIndex = -1;[m[41m
[m
[32m+[m[32m      }[m[41m
[m
[32m+[m[41m
[m
[32m+[m[32m      // fill it with your HTML[m[41m
[m
[32m+[m[32m      tmpEl.innerHTML = html;[m[41m
[m
[32m+[m[41m
[m
[32m+[m[32m      // append the temporary node to the DOM[m[41m
[m
[32m+[m[32m      document.body.appendChild(tmpEl);[m[41m
[m
[32m+[m[41m
[m
[32m+[m[32m      // select the newly added node[m[41m
[m
[32m+[m[32m      var range = document.createRange();[m[41m
[m
[32m+[m[32m      range.selectNode(tmpEl);[m[41m
[m
[32m+[m[32m      window.getSelection().addRange(range);[m[41m
[m
[32m+[m[41m
[m
[32m+[m[32m      // copy[m[41m
[m
[32m+[m[32m      document.execCommand("copy");[m[41m
[m
[32m+[m[41m
[m
[32m+[m[32m      // and remove the element immediately[m[41m
[m
[32m+[m[32m      document.body.removeChild(tmpEl);[m[41m
[m
[32m+[m[32m  }[m[41m
[m
[32m+[m[41m
[m
[32m+[m[32m  document.getElementById("foo").addEventListener("click", function () {[m[41m
[m
[32m+[m[32m      html2cli"pboard();[m[41m
[m
[32m+[m[32m  });"[m[41m
[m
[32m+[m[32m  */[m[41m
[m
[32m+[m[32m  </script>[m[41m
