[32m+[m[32m/* jshint laxbreak: true */[m
[32m+[m[32m/* jshint maxcomplexity:17 */[m
[32m+[m[32m/* global define */[m
[32m+[m
[32m+[m[32m// UMD (Universal Module Definition) patterns for JavaScript modules that work everywhere.[m
[32m+[m[32m// https://github.com/umdjs/umd/blob/master/templates/jqueryPlugin.js[m
[32m+[m[32m(function (factory, jQuery, Zepto) {[m
[32m+[m
[32m+[m[32m    if (typeof define === 'function' && define.amd) {[m
[32m+[m[32m        define(['jquery'], factory);[m
[32m+[m[32m    } else if (typeof exports === 'object') {[m
[32m+[m[32m        module.exports = factory(require('jquery'));[m
[32m+[m[32m    } else {[m
[32m+[m[32m        factory(jQuery || Zepto);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m}(function ($) {[m
[32m+[m[32m    'use strict';[m
[32m+[m
[32m+[m[32m    var Mask = function (el, mask, options) {[m
[32m+[m
[32m+[m[32m        var p = {[m
[32m+[m[32m            invalid: [],[m
[32m+[m[32m            getCaret: function () {[m
[32m+[m[32m                try {[m
[32m+[m[32m                    var sel,[m
[32m+[m[32m                        pos = 0,[m
[32m+[m[32m                        ctrl = el.get(0),[m
[32m+[m[32m                        dSel = document.selection,[m
[32m+[m[32m                        cSelStart = ctrl.selectionStart;[m
[32m+[m
[32m+[m[32m                    // IE Support[m
[32m+[m[32m                    if (dSel && navigator.appVersion.indexOf('MSIE 10') === -1) {[m
[32m+[m[32m                        sel = dSel.createRange();[m
[32m+[m[32m                        sel.moveStart('character', -p.val().length);[m
[32m+[m[32m                        pos = sel.text.length;[m
[32m+[m[32m                    }[m
[32m+[m[32m                    // Firefox support[m
[32m+[m[32m                    else if (cSelStart || cSelStart === '0') {[m
[32m+[m[32m                        pos = cSelStart;[m
[32m+[m[32m                    }[m
[32m+[m
[32m+[m[32m                    return pos;[m
[32m+[m[32m                } catch (e) {}[m
[32m+[m[32m            },[m
[32m+[m[32m            setCaret: function(pos) {[m
[32m+[m[32m                try {[m
[32m+[m[32m                    if (el.is(':focus')) {[m
[32m+[m[32m                        var range, ctrl = el.get(0);[m
[32m+[m
[32m+[m[32m                        // Firefox, WebKit, etc..[m
[32m+[m[32m                        if (ctrl.setSelectionRange) {[m
[32m+[m[32m                            ctrl.setSelectionRange(pos, pos);[m
[32m+[m[32m                        } else { // IE[m
[32m+[m[32m                            range = ctrl.createTextRange();[m
[32m+[m[32m                            range.collapse(true);[m
[32m+[m[32m                            range.moveEnd('character', pos);[m
[32m+[m[32m                            range.moveStart('character', pos);[m
[32m+[m[32m                            range.select();[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                } catch (e) {}[m
[32m+[m[32m            },[m
[32m+[m[32m            events: function() {[m
[32m+[m[32m                el[m
[32m+[m[32m                .on('keydown.mask', function(e) {[m
[32m+[m[32m                    el.data('mask-keycode', e.keyCode || e.which);[m
[32m+[m[32m                    el.data('mask-previus-value', el.val());[m
[32m+[m[32m                    el.data('mask-previus-caret-pos', p.getCaret());[m
[32m+[m[32m                    p.maskDigitPosMapOld = p.maskDigitPosMap;[m
[32m+[m[32m                })[m
[32m+[m[32m                .on($.jMaskGlobals.useInput ? 'input.mask' : 'keyup.mask', p.behaviour)[m
[32m+[m[32m                .on('paste.mask drop.mask', function() {[m
[32m+[m[32m                    setTimeout(function() {[m
[32m+[m[32m                        el.keydown().keyup();[m
[32m+[m[32m                    }, 100);[m
[32m+[m[32m                })[m
[32m+[m[32m                .on('change.mask', function(){[m
[32m+[m[32m                    el.data('changed', true);[m
[32m+[m[32m                })[m
[32m+[m[32m                .on('blur.mask', function(){[m
[32m+[m[32m                    if (oldValue !== p.val() && !el.data('changed')) {[m
[32m+[m[32m                        el.trigger('change');[m
[32m+[m[32m                    }[m
[32m+[m[32m                    el.data('changed', false);[m
[32m+[m[32m                })[m
[32m+[m[32m                // it's very important that this callback remains in this position[m
[32m+[m[32m                // otherwhise oldValue it's going to work buggy[m
[32m+[m[32m                .on('blur.mask', function() {[m
[32m+[m[32m                    oldValue = p.val();[m
[32m+[m[32m                })[m
[32m+[m[32m                // select all text on focus[m
[32m+[m[32m                .on('focus.mask', function (e) {[m
[32m+[m[32m                    if (options.selectOnFocus === true) {[m
[32m+[m[32m                        $(e.target).select();[m
[32m+[m[32m                    }[m
[32m+[m[32m                })[m
[32m+[m[32m                // clear the value if it not complete the mask[m
[32m+[m[32m                .on('focusout.mask', function() {[m
[32m+[m[32m                    if (options.clearIfNotMatch && !regexMask.test(p.val())) {[m
[32m+[m[32m                       p.val('');[m
[32m+[m[32m                   }[m
[32m+[m[32m                });[m
[32m+[m[32m            },[m
[32m+[m[32m            getRegexMask: function() {[m
[32m+[m[32m                var maskChunks = [], translation, pattern, optional, recursive, oRecursive, r;[m
[32m+[m
[32m+[m[32m                for (var i = 0; i < mask.length; i++) {[m
[32m+[m[32m                    translation = jMask.translation[mask.charAt(i)];[m
[32m+[m
[32m+[m[32m                    if (translation) {[m
[32m+[m
[32m+[m[32m                        pattern = translation.pattern.toString().replace(/.{1}$|^.{1}/g, '');[m
[32m+[m[32m                        optional = translation.optional;[m
[32m+[m[32m                        recursive = translation.recursive;[m
[32m+[m
[32m+[m[32m                        if (recursive) {[m
[32m+[m[32m                            maskChunks.push(mask.charAt(i));[m
[32m+[m[32m                            oRecursive = {digit: mask.charAt(i), pattern: pattern};[m
[32m+[m[32m                        } else {[m
[32m+[m[32m                            maskChunks.push(!optional && !recursive ? pattern : (pattern + '?'));[m
[32m+[m[32m                        }[m
[32m+[m
[32m+[m[32m                    } else {[m
[32m+[m[32m                        maskChunks.push(mask.charAt(i).replace(/[-\/\\^$*+?.()|[\]{}]/g, '\\$&'));[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                r = maskChunks.join('');[m
[32m+[m
[32m+[m[32m                if (oRecursive) {[m
[32m+[m[32m                    r = r.replace(new RegExp('(' + oRecursive.digit + '(.*' + oRecursive.digit + ')?)'), '($1)?')[m
[32m+[m[32m                         .replace(new RegExp(oRecursive.digit, 'g'), oRecursive.pattern);[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                return new RegExp(r);[m
[32m+[m[32m            },[m
[32m+[m[32m            destroyEvents: function() {[m
[32m+[m[32m                el.off(['input', 'keydown', 'keyup', 'paste', 'drop', 'blur', 'focusout', ''].join('.mask '));[m
[32m+[m[32m            },[m
[32m+[m[32m            val: function(v) {[m
[32m+[m[32m                var isInput = el.is('input'),[m
[32m+[m[32m                    method = isInput ? 'val' : 'text',[m
[32m+[m[32m                    r;[m
[32m+[m
[32m+[m[32m                if (arguments.length > 0) {[m
[32m+[m[32m                    if (el[method]() !== v) {[m
[32m+[m[32m                        el[method](v);[m
[32m+[m[32m                    }[m
[32m+[m[32m                    r = el;[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    r = el[method]();[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                return r;[m
[32m+[m[32m            },[m
[32m+[m[32m            calculateCaretPosition: function() {[m
[32m+[m[32m                var oldVal = el.data('mask-previus-value') || '',[m
[32m+[m[32m                    newVal = p.getMasked(),[m
[32m+[m[32m                    caretPosNew = p.getCaret();[m
[32m+[m[32m                if (oldVal !== newVal) {[m
[32m+[m[32m                    var caretPosOld = el.data('mask-previus-caret-pos') || 0,[m
[32m+[m[32m                        newValL = newVal.length,[m
[32m+[m[32m                        oldValL = oldVal.length,[m
[32m+[m[32m                        maskDigitsBeforeCaret = 0,[m
[32m+[m[32m                        maskDigitsAfterCaret = 0,[m
[32m+[m[32m                        maskDigitsBeforeCaretAll = 0,[m
[32m+[m[32m                        maskDigitsBeforeCaretAllOld = 0,[m
[32m+[m[32m                        i = 0;[m
[32m+[m
[32m+[m[32m                    for (i = caretPosNew; i < newValL; i++) {[m
[32m+[m[32m                        if (!p.maskDigitPosMap[i]) {[m
[32m+[m[32m                            break;[m
[32m+[m[32m                        }[m
[32m+[m[32m                        maskDigitsAfterCaret++;[m
[32m+[m[32m                    }[m
[32m+[m
[32m+[m[32m                    for (i = caretPosNew - 1; i >= 0; i--) {[m
[32m+[m[32m                        if (!p.maskDigitPosMap[i]) {[m
[32m+[m[32m                            break;[m
[32m+[m[32m                        }[m
[32m+[m[32m                        maskDigitsBeforeCaret++;[m
[32m+[m[32m                    }[m
[32m+[m
[32m+[m[32m                    for (i = caretPosNew - 1; i >= 0; i--) {[m
[32m+[m[32m                        if (p.maskDigitPosMap[i]) {[m
[32m+[m[32m                            maskDigitsBeforeCaretAll++;[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m
[32m+[m[32m                    for (i = caretPosOld - 1; i >= 0; i--) {[m
[32m+[m[32m                        if (p.maskDigitPosMapOld[i]) {[m
[32m+[m[32m                            maskDigitsBeforeCaretAllOld++;[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m
[32m+[m[32m                    // if the cursor is at the end keep it there[m
[32m+[m[32m                    if (caretPosNew > oldValL) {[m
[32m+[m[32m                      caretPosNew = newValL * 10;[m
[32m+[m[32m                    } else if (caretPosOld >= caretPosNew && caretPosOld !== oldValL) {[m
[32m+[m[32m                        if (!p.maskDigitPosMapOld[caretPosNew])  {[m
[32m+[m[32m                          var caretPos = caretPosNew;[m
[32m+[m[32m                          caretPosNew -= maskDigitsBeforeCaretAllOld - maskDigitsBeforeCaretAll;[m
[32m+[m[32m                          caretPosNew -= maskDigitsBeforeCaret;[m
[32m+[m[32m                          if (p.maskDigitPosMap[caretPosNew])  {[m
[32m+[m[32m                            caretPosNew = caretPos;[m
[32m+[m[32m                          }[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                    else if (caretPosNew > caretPosOld) {[m
[32m+[m[32m                        caretPosNew += maskDigitsBeforeCaretAll - maskDigitsBeforeCaretAllOld;[m
[32m+[m[32m                        caretPosNew += maskDigitsAfterCaret;[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                return caretPosNew;[m
[32m+[m[32m            },[m
[32m+[m[32m            behaviour: function(e) {[m
[32m+[m[32m                e = e || window.event;[m
[32m+[m[32m                p.invalid = [];[m
[32m+[m
[32m+[m[32m                var keyCode = el.data('mask-keycode');[m
[32m+[m
[32m+[m[32m                if ($.inArray(keyCode, jMask.byPassKeys) === -1) {[m
[32m+[m[32m                    var newVal = p.getMasked(),[m
[32m+[m[32m                        caretPos = p.getCaret();[m
[32m+[m
[32m+[m[32m                    // this is a compensation to devices/browsers that don't compensate[m
[32m+[m[32m                    // caret positioning the right way[m
[32m+[m[32m                    setTimeout(function() {[m
[32m+[m[32m                      p.setCaret(p.calculateCaretPosition());[m
[32m+[m[32m                    }, $.jMaskGlobals.keyStrokeCompensation);[m
[32m+[m
[32m+[m[32m                    p.val(newVal);[m
[32m+[m[32m                    p.setCaret(caretPos);[m
[32m+[m[32m                    return p.callbacks(e);[m
[32m+[m[32m                }[m
[32m+[m[32m            },[m
[32m+[m[32m            getMasked: function(skipMaskChars, val) {[m
[32m+[m[32m                var buf = [],[m
[32m+[m[32m                    value = val === undefined ? p.val() : val + '',[m
[32m+[m[32m                    m = 0, maskLen = mask.length,[m
[32m+[m[32m                    v = 0, valLen = value.length,[m
[32m+[m[32m                    offset = 1, addMethod = 'push',[m
[32m+[m[32m                    resetPos = -1,[m
[32m+[m[32m                    maskDigitCount = 0,[m
[32m+[m[32m                    maskDigitPosArr = [],[m
[32m+[m[32m                    lastMaskChar,[m
[32m+[m[32m                    check;[m
[32m+[m
[32m+[m[32m                if (options.reverse) {[m
[32m+[m[32m                    addMethod = 'unshift';[m
[32m+[m[32m                    offset = -1;[m
[32m+[m[32m                    lastMaskChar = 0;[m
[32m+[m[32m                    m = maskLen - 1;[m
[32m+[m[32m                    v = valLen - 1;[m
[32m+[m[32m                    check = function () {[m
[32m+[m[32m                        return m > -1 && v > -1;[m
[32m+[m[32m                    };[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    lastMaskChar = maskLen - 1;[m
[32m+[m[32m                    check = function () {[m
[32m+[m[32m                        return m < maskLen && v < valLen;[m
[32m+[m[32m                    };[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                var lastUntranslatedMaskChar;[m
[32m+[m[32m                while (check()) {[m
[32m+[m[32m                    var maskDigit = mask.charAt(m),[m
[32m+[m[32m                        valDigit = value.charAt(v),[m
[32m+[m[32m                        translation = jMask.translation[maskDigit];[m
[32m+[m
[32m+[m[32m                    if (translation) {[m
[32m+[m[32m                        if (valDigit.match(translation.pattern)) {[m
[32m+[m[32m                            buf[addMethod](valDigit);[m
[32m+[m[32m                             if (translation.recursive) {[m
[32m+[m[32m                                if (resetPos === -1) {[m
[32m+[m[32m                                    resetPos = m;[m
[32m+[m[32m                                } else if (m === lastMaskChar && m !== resetPos) {[m
[32m+[m[32m                                    m = resetPos - offset;[m
[32m+[m[32m                                }[m
[32m+[m
[32m+[m[32m                                if (lastMaskChar === resetPos) {[m
[32m+[m[32m                                    m -= offset;[m
[32m+[m[32m                                }[m
[32m+[m[32m                            }[m
[32m+[m[32m                            m += offset;[m
[32m+[m[32m                        } else if (valDigit === lastUntranslatedMaskChar) {[m
[32m+[m[32m                            // matched the last untranslated (raw) mask character that we encountered[m
[32m+[m[32m                            // likely an insert offset the mask character from the last entry; fall[m
[32m+[m[32m                            // through and only increment v[m
[32m+[m[32m                            maskDigitCount--;[m
[32m+[m[32m                            lastUntranslatedMaskChar = undefined;[m
[32m+[m[32m                        } else if (translation.optional) {[m
[32m+[m[32m                            m += offset;[m
[32m+[m[32m                            v -= offset;[m
[32m+[m[32m                        } else if (translation.fallback) {[m
[32m+[m[32m                            buf[addMethod](translation.fallback);[m
[32m+[m[32m                            m += offset;[m
[32m+[m[32m                            v -= offset;[m
[32m+[m[32m                        } else {[m
[32m+[m[32m                          p.invalid.push({p: v, v: valDigit, e: translation.pattern});[m
[32m+[m[32m                        }[m
[32m+[m[32m                        v += offset;[m
[32m+[m[32m                    } else {[m
[32m+[m[32m                        if (!skipMaskChars) {[m
[32m+[m[32m                            buf[addMethod](maskDigit);[m
[32m+[m[32m                        }[m
[32m+[m
[32m+[m[32m                        if (valDigit === maskDigit) {[m
[32m+[m[32m                            maskDigitPosArr.push(v);[m
[32m+[m[32m                            v += offset;[m
[32m+[m[32m                        } else {[m
[32m+[m[32m                            lastUntranslatedMaskChar = maskDigit;[m
[32m+[m[32m                            maskDigitPosArr.push(v + maskDigitCount);[m
[32m+[m[32m                            maskDigitCount++;[m
[32m+[m[32m                        }[m
[32m+[m
[32m+[m[32m                        m += offset;[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                var lastMaskCharDigit = mask.charAt(lastMaskChar);[m
[32m+[m[32m                if (maskLen === valLen + 1 && !jMask.translation[lastMaskCharDigit]) {[m
[32m+[m[32m                    buf.push(lastMaskCharDigit);[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                var newVal = buf.join('');[m
[32m+[m[32m                p.mapMaskdigitPositions(newVal, maskDigitPosArr, valLen);[m
[32m+[m[32m                return newVal;[m
[32m+[m[32m            },[m
[32m+[m[32m            mapMaskdigitPositions: function(newVal, maskDigitPosArr, valLen) {[m
[32m+[m[32m              var maskDiff = options.reverse ? newVal.length - valLen : 0;[m
[32m+[m[32m              p.maskDigitPosMap = {};[m
[32m+[m[32m              for (var i = 0; i < maskDigitPosArr.length; i++) {[m
[32m+[m[32m                p.maskDigitPosMap[maskDigitPosArr[i] + maskDiff] = 1;[m
[32m+[m[32m              }[m
[32m+[m[32m            },[m
[32m+[m[32m            callbacks: function (e) {[m
[32m+[m[32m                var val = p.val(),[m
[32m+[m[32m                    changed = val !== oldValue,[m
[32m+[m[32m                    defaultArgs = [val, e, el, options],[m
[32m+[m[32m                    callback = function(name, criteria, args) {[m
[32m+[m[32m                        if (typeof options[name] === 'function' && criteria) {[m
[32m+[m[32m                            options[name].apply(this, args);[m
[32m+[m[32m                        }[m
[32m+[m[32m                    };[m
[32m+[m
[32m+[m[32m                callback('onChange', changed === true, defaultArgs);[m
[32m+[m[32m                callback('onKeyPress', changed === true, defaultArgs);[m
[32m+[m[32m                callback('onComplete', val.length === mask.length, defaultArgs);[m
[32m+[m[32m                callback('onInvalid', p.invalid.length > 0, [val, e, el, p.invalid, options]);[m
[32m+[m[32m            }[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        el = $(el);[m
[32m+[m[32m        var jMask = this, oldValue = p.val(), regexMask;[m
[32m+[m
[32m+[m[32m        mask = typeof mask === 'function' ? mask(p.val(), undefined, el,  options) : mask;[m
[32m+[m
[32m+[m[32m        // public methods[m
[32m+[m[32m        jMask.mask = mask;[m
[32m+[m[32m        jMask.options = options;[m
[32m+[m[32m        jMask.remove = function() {[m
[32m+[m[32m            var caret = p.getCaret();[m
[32m+[m[32m            if (jMask.options.placeholder) {[m
[32m+[m[32m                el.removeAttr('placeholder');[m
[32m+[m[32m            }[m
[32m+[m[32m            if (el.data('mask-maxlength')) {[m
[32m+[m[32m                el.removeAttr('maxlength');[m
[32m+[m[32m            }[m
[32m+[m[32m            p.destroyEvents();[m
[32m+[m[32m            p.val(jMask.getCleanVal());[m
[32m+[m[32m            p.setCaret(caret);[m
[32m+[m[32m            return el;[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        // get value without mask[m
[32m+[m[32m        jMask.getCleanVal = function() {[m
[32m+[m[32m           return p.getMasked(true);[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        // get masked value without the value being in the input or element[m
[32m+[m[32m        jMask.getMaskedVal = function(val) {[m
[32m+[m[32m           return p.getMasked(false, val);[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m       jMask.init = function(onlyMask) {[m
[32m+[m[32m            onlyMask = onlyMask || false;[m
[32m+[m[32m            options = options || {};[m
[32m+[m
[32m+[m[32m            jMask.clearIfNotMatch  = $.jMaskGlobals.clearIfNotMatch;[m
[32m+[m[32m            jMask.byPassKeys       = $.jMaskGlobals.byPassKeys;[m
[32m+[m[32m            jMask.translation      = $.extend({}, $.jMaskGlobals.translation, options.translation);[m
[32m+[m
[32m+[m[32m            jMask = $.extend(true, {}, jMask, options);[m
[32m+[m
[32m+[m[32m            regexMask = p.getRegexMask();[m
[32m+[m
[32m+[m[32m            if (onlyMask) {[m
[32m+[m[32m                p.events();[m
[32m+[m[32m                p.val(p.getMasked());[m
[32m+[m[32m            } else {[m
[32m+[m[32m                if (options.placeholder) {[m
[32m+[m[32m                    el.attr('placeholder' , options.placeholder);[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                // this is necessary, otherwise if the user submit the form[m
[32m+[m[32m                // and then press the "back" button, the autocomplete will erase[m
[32m+[m[32m                // the data. Works fine on IE9+, FF, Opera, Safari.[m
[32m+[m[32m                if (el.data('mask')) {[m
[32m+[m[32m                  el.attr('autocomplete', 'off');[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                // detect if is necessary let the user type freely.[m
[32m+[m[32m                // for is a lot faster than forEach.[m
[32m+[m[32m                for (var i = 0, maxlength = true; i < mask.length; i++) {[m
[32m+[m[32m                    var translation = jMask.translation[mask.charAt(i)];[m
[32m+[m[32m                    if (translation && translation.recursive) {[m
[32m+[m[32m                        maxlength = false;[m
[32m+[m[32m                        break;[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                if (maxlength) {[m
[32m+[m[32m                    el.attr('maxlength', mask.length).data('mask-maxlength', true);[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                p.destroyEvents();[m
[32m+[m[32m                p.events();[m
[32m+[m
[32m+[m[32m                var caret = p.getCaret();[m
[32m+[m[32m                p.val(p.getMasked());[m
[32m+[m[32m                p.setCaret(caret);[m
[32m+[m[32m            }[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        jMask.init(!el.is('input'));[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    $.maskWatchers = {};[m
[32m+[m[32m    var HTMLAttributes = function () {[m
[32m+[m[32m        var input = $(this),[m
[32m+[m[32m            options = {},[m
[32m+[m[32m            prefix = 'data-mask-',[m
[32m+[m[32m            mask = input.attr('data-mask');[m
[32m+[m
[32m+[m[32m        if (input.attr(prefix + 'reverse')) {[m
[32m+[m[32m            options.reverse = true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (input.attr(prefix + 'clearifnotmatch')) {[m
[32m+[m[32m            options.clearIfNotMatch = true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (input.attr(prefix + 'selectonfocus') === 'true') {[m
[32m+[m[32m           options.selectOnFocus = true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (notSameMaskObject(input, mask, options)) {[m
[32m+[m[32m            return input.data('mask', new Mask(this, mask, options));[m
[32m+[m[32m        }[m
[32m+[m[32m    },[m
[32m+[m[32m    notSameMaskObject = function(field, mask, options) {[m
[32m+[m[32m        options = options || {};[m
[32m+[m[32m        var maskObject = $(field).data('mask'),[m
[32m+[m[32m            stringify = JSON.stringify,[m
[32m+[m[32m            value = $(field).val() || $(field).text();[m
[32m+[m[32m        try {[m
[32m+[m[32m            if (typeof mask === 'function') {[m
[32m+[m[32m                mask = mask(value);[m
[32m+[m[32m            }[m
[32m+[m[32m            return typeof maskObject !== 'object' || stringify(maskObject.options) !== stringify(options) || maskObject.mask !== mask;[m
[32m+[m[32m        } catch (e) {}[m
[32m+[m[32m    },[m
[32m+[m[32m    eventSupported = function(eventName) {[m
[32m+[m[32m        var el = document.createElement('div'), isSupported;[m
[32m+[m
[32m+[m[32m        eventName = 'on' + eventName;[m
[32m+[m[32m        isSupported = (eventName in el);[m
[32m+[m
[32m+[m[32m        if ( !isSupported ) {[m
[32m+[m[32m            el.setAttribute(eventName, 'return;');[m
[32m+[m[32m            isSupported = typeof el[eventName] === 'function';[m
[32m+[m[32m        }[m
[32m+[m[32m        el = null;[m
[32m+[m
[32m+[m[32m        return isSupported;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    $.fn.mask = function(mask, options) {[m
[32m+[m[32m        options = options || {};[m
[32m+[m[32m        var selector = this.selector,[m
[32m+[m[32m            globals = $.jMaskGlobals,[m
[32m+[m[32m            interval = globals.watchInterval,[m
[32m+[m[32m            watchInputs = options.watchInputs || globals.watchInputs,[m
[32m+[m[32m            maskFunction = function() {[m
[32m+[m[32m                if (notSameMaskObject(this, mask, options)) {[m
[32m+[m[32m                    return $(this).data('mask', new Mask(this, mask, options));[m
[32m+[m[32m                }[m
[32m+[m[32m            };[m
[32m+[m
[32m+[m[32m        $(this).each(maskFunction);[m
[32m+[m
[32m+[m[32m        if (selector && selector !== '' && watchInputs) {[m
[32m+[m[32m            clearInterval($.maskWatchers[selector]);[m
[32m+[m[32m            $.maskWatchers[selector] = setInterval(function(){[m
[32m+[m[32m                $(document).find(selector).each(maskFunction);[m
[32m+[m[32m            }, interval);[m
[32m+[m[32m        }[m
[32m+[m[32m        return this;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    $.fn.masked = function(val) {[m
[32m+[m[32m        return this.data('mask').getMaskedVal(val);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    $.fn.unmask = function() {[m
[32m+[m[32m        clearInterval($.maskWatchers[this.selector]);[m
[32m+[m[32m        delete $.maskWatchers[this.selector];[m
[32m+[m[32m        return this.each(function() {[m
[32m+[m[32m            var dataMask = $(this).data('mask');[m
[32m+[m[32m            if (dataMask) {[m
[32m+[m[32m                dataMask.remove().removeData('mask');[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    $.fn.cleanVal = function() {[m
[32m+[m[32m        return this.data('mask').getCleanVal();[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    $.applyDataMask = function(selector) {[m
[32m+[m[32m        selector = selector || $.jMaskGlobals.maskElements;[m
[32m+[m[32m        var $selector = (selector instanceof $) ? selector : $(selector);[m
[32m+[m[32m        $selector.filter($.jMaskGlobals.dataMaskAttr).each(HTMLAttributes);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    var globals = {[m
[32m+[m[32m        maskElements: 'input,td,span,div',[m
[32m+[m[32m        dataMaskAttr: '*[data-mask]',[m
[32m+[m[32m        dataMask: true,[m
[32m+[m[32m        watchInterval: 300,[m
[32m+[m[32m        watchInputs: true,[m
[32m+[m[32m        keyStrokeCompensation: 10,[m
[32m+[m[32m        // old versions of chrome dont work great with input event[m
[32m+[m[32m        useInput: !/Chrome\/[2-4][0-9]|SamsungBrowser/.test(window.navigator.userAgent) && eventSupported('input'),[m
[32m+[m[32m        watchDataMask: false,[m
[32m+[m[32m        byPassKeys: [9, 16, 17, 18, 36, 37, 38, 39, 40, 91],[m
[32m+[m[32m        translation: {[m
[32m+[m[32m            '0': {pattern: /\d/},[m
[32m+[m[32m            '9': {pattern: /\d/, optional: true},[m
[32m+[m[32m            '#': {pattern: /\d/, recursive: true},[m
[32m+[m[32m            'A': {pattern: /[a-zA-Z0-9]/},[m
[32m+[m[32m            'S': {pattern: /[a-zA-Z]/}[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    $.jMaskGlobals = $.jMaskGlobals || {};[m
[32m+[m[32m    globals = $.jMaskGlobals = $.extend(true, {}, globals, $.jMaskGlobals);[m
[32m+[m
[32m+[m[32m    // looking for inputs with data-mask attribute[m
[32m+[m[32m    if (globals.dataMask) {[m
[32m+[m[32m        $.applyDataMask();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    setInterval(function() {[m
[32m+[m[32m        if ($.jMaskGlobals.watchDataMask) {[m
[32m+[m[32m            $.applyDataMask();[m
[32m+[m[32m        }[m
[32m+[m[32m    }, globals.watchInterval);[m
[32m+[m[32m}, window.jQuery, window.Zepto));[m
