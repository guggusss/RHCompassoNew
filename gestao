[32m+[m
[32m+[m[32mfunction isValidPerfilAllAccess($saida)[m
[32m+[m[32m{[m
[32m+[m[32m    for ($i = 0; $i < $saida[0]['memberof']['count']; $i++) {[m
[32m+[m[32m        $grupoAd = $saida[0]['memberof'][$i];[m
[32m+[m[32m        $textGroup = preg_split('/=|\,/', $grupoAd);[m
[32m+[m[41m        [m
[32m+[m[32m        $grupo = $textGroup[1];[m
[32m+[m[41m                [m
[32m+[m[32m        if ($grupo == "Diretoria" || $grupo == "Compasso - RH contratacoes") {[m
[32m+[m[32m            return $grupo;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    return false;[m
[32m+[m[32m}[m[41m [m
[32m+[m
[32m+[m[32mfunction isRH($saida){[m
[32m+[m[32m    for ($i = 0; $i < $saida[0]['memberof']['count']; $i++) {[m
[32m+[m[32m        $grupoAd = $saida[0]['memberof'][$i];[m
[32m+[m[32m        $textGroup = preg_split('/=|\,/', $grupoAd);[m
[32m+[m
[32m+[m[32m        $grupo = $textGroup[1];[m
[32m+[m[41m        [m
[32m+[m[32m        if ($grupo == "Departamento de Recursos Humanos") {[m
[32m+[m[32m            return $grupo;[m
[32m+[m[32m        }[m[41m      [m
[32m+[m[32m    }[m
[32m+[m[32m    return false;[m
[32m+[m[32m}[m
[32m+[m[32mfunction isGestor($saida){[m
[32m+[m[32m    for ($i = 0; $i < $saida[0]['memberof']['count']; $i++) {[m
[32m+[m[32m        $grupoAd = $saida[0]['memberof'][$i];[m
[32m+[m[32m        $textGroup = preg_split('/=|\,/', $grupoAd);[m
[32m+[m
[32m+[m[32m        $grupo = $textGroup[1];[m
[32m+[m[41m        [m
[32m+[m[32m        if (strpos($grupo, "Gestores") !== false || $grupo == "Gestores") {[m
[32m+[m[32m            return $grupo;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    return false;[m
[32m+[m[32m}[m
[32m+[m[32mfunction isSuporteInterno($saida){[m
[32m+[m[32m    for ($i = 0; $i < $saida[0]['memberof']['count']; $i++) {[m
[32m+[m[32m        $grupoAd = $saida[0]['memberof'][$i];[m
[32m+[m[32m        $textGroup = preg_split('/=|\,/', $grupoAd);[m
[32m+[m
[32m+[m[32m        $grupo = $textGroup[1];[m
[32m+[m[41m        [m
[32m+[m[32m        if ($grupo == "Suporte Interno") {[m
[32m+[m[32m            return $grupo;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    return false;[m
[32m+[m[32m}[m
[32m+[m[32mfunction isApoioSede($saida){[m
[32m+[m[32m    for ($i = 0; $i < $saida[0]['memberof']['count']; $i++) {[m
[32m+[m[32m        $grupoAd = $saida[0]['memberof'][$i];[m
[32m+[m[32m        $textGroup = preg_split('/=|\,/', $grupoAd);[m
[32m+[m
[32m+[m[32m        if (preg_match('/Compasso - RH Integra√ß√£o/', $textGroup[1], $grupoAd)) {[m
[32m+[m[32m            $grupo = $grupoAd[0];[m
[32m+[m[32m        } else {[m
[32m+[m[32m            $grupo = $textGroup[1];[m
[32m+[m[32m        }[m
[32m+[m[32m        if ($grupo == "Compasso - RH Integra√ß√£o") {[m
[32m+[m[32m            return $grupo;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    return false;[m
[32m+[m[32m}[m
