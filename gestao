[32m+[m[32m<?php[m
[32m+[m[32mrequire_once('../../db/serverLDAP.php');[m
[32m+[m
[32m+[m[32msession_start();[m
[32m+[m
[32m+[m[32m$usuario = $_POST['usuario'];[m
[32m+[m[32m$senha = $_POST['senha'];[m
[32m+[m
[32m+[m[32mif ($usuario == NULL || $senha == NULL) {[m
[32m+[m[32m    header("Location:./login.php?erro=fail");[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m$dominio = "pampa.compasso";[m
[32m+[m
[32m+[m[32m// Versao do protocolo[m[41m       [m
[32m+[m[32mldap_set_option($link, LDAP_OPT_PROTOCOL_VERSION, 3);[m
[32m+[m[32m// Usara as referencias do servidor AD, neste caso nao[m
[32m+[m[32mldap_set_option($link, LDAP_OPT_REFERRALS, 0);[m
[32m+[m
[32m+[m[32m$r = @ldap_bind($link, $usuario . '@' . $dominio, $senha);[m
[32m+[m
[32m+[m[32m$filtro = "(samaccountname=" . $usuario . ")";[m
[32m+[m[32m$justthese = array("*");[m
[32m+[m[32m$res = ldap_search($link, "dc=pampa,dc=compasso", $filtro, $justthese);[m
[32m+[m[32m$saida = ldap_get_entries($link, $res);[m
[32m+[m[32m$nomeGrupo = null;[m
[32m+[m
[32m+[m[32mif ($saida['count'] > 0) {[m
[32m+[m[32m    $groupPerfil = isValidPerfilAllAccess($saida);[m
[32m+[m[32m    if($groupPerfil != false){[m
[32m+[m[32m        $_SESSION["InfoUser"] = $saida;[m
[32m+[m[32m        $_SESSION["grupo"] = $groupPerfil;[m
[32m+[m[41m        [m
[32m+[m[32m        header("location:../../telas/index.php");[m
[32m+[m[32m    } else{[m
[32m+[m[32m        $groupPerfil = isRH($saida);[m
[32m+[m[32m        if ($groupPerfil) {[m
[32m+[m[32m            $_SESSION["InfoUser"] = $saida;[m
[32m+[m[32m            $_SESSION["grupo"] = $groupPerfil;[m
[32m+[m[41m            [m
[32m+[m[32m            header("location:../../telas/index.php");[m
[32m+[m[32m        } else{[m
[32m+[m[32m            $groupPerfil = isGestor($saida);[m
[32m+[m[32m            if($groupPerfil){[m
[32m+[m[32m                $_SESSION["InfoUser"] = $saida;[m
[32m+[m[32m                $_SESSION["grupo"] = $groupPerfil;[m
[32m+[m[41m                [m
[32m+[m[32m                header("location:../../telas/index.php");[m
[32m+[m[32m            }else{[m
[32m+[m[32m                $groupPerfil = isSuporteInterno($saida);[m
[32m+[m[32m                if($groupPerfil){[m
