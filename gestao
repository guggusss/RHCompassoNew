[32m+[m[32m    function mtel(v){[m
[32m+[m[32m    v=v.replace(/\D/g,"");             //Remove tudo o que n√£o √© d√≠gito[m
[32m+[m[32m    v=v.replace(/^(\d{2})(\d)/g,"($1) $2"); //Coloca par√™nteses em volta dos dois primeiros d√≠gitos[m
[32m+[m[32m    v=v.replace(/(\d)(\d{4})$/,"$1-$2");    //Coloca h√≠fen entre o quarto e o quinto d√≠gitos[m
[32m+[m[32m    return v;[m
[32m+[m[32m    }[m
